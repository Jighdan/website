---
import { Image } from 'astro:assets';
import { AstroSeo } from '@astrolib/seo';
import Layout from '~/layouts/Default.astro';
import media from '~/assets/images/hero.jpg';
---

<Layout>
	<AstroSeo
	title='Reinny Almonte Ramos'
	description="Reinny's Personal Space"
	slot='seo'
/>

	<Image src={media} alt="the first picture Reinny took with a film camera" class="object-cover object-center w-full h-full select-none" />
</Layout>
